{
  "name": "svelte-css-rune",
  "module": "dist/esm/index.js",
  "author": "Jan Nitschke",
  "license": "MIT",
  "readme": "README.md",
  "description": "$css rune for svelte. Allows you to pass classes between your svelte components.",
  "keywords": [
    "svelte",
    "css",
    "rune",
    "classes",
    "preprocessor",
    "global"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JanNitschke/svelte-css-rune"
  },
  "type": "module",
  "version": "0.1.4",
  "scripts": {
    "prebuild": "rm -rf dist/",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --module commonjs --target es6 --outDir dist/cjs --declaration true",
    "build:esm": "tsc --module esnext --target esnext --outDir dist/esm",
    "dev": "npm run build:esm -- -w"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "estree-walker": "^3.0.3",
    "happy-dom": "^16.6.0",
    "module-from-string": "^3.3.1",
    "typescript": "^5.7.3",
    "zimmerframe": "^1.1.2"
  },
  "peerDependencies": {
    "svelte": "^5.15.0"
  },
  "dependencies": {
    "magic-string": "^0.30.17"
  },
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  }
}